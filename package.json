{
  "name": "plutoapp-new-frontend",
  "version": "0.1.0",
  "main": "public/electron.js",
  "author": "Pluto Dev Team",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "animate.css": "^3.7.0",
    "axios": "^0.19.2",
    "electron-disable-minimize": "^2.0.2",
    "electron-is-dev": "^1.2.0",
    "electron-titlebar": "0.0.3",
    "firebase": "^7.14.2",
    "lodash": "^4.17.15",
    "overmind": "^23.0.1",
    "overmind-react": "^24.0.1",
    "qs": "^6.9.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-spinners": "^0.8.1",
    "tailwindcss": "^1.3.5",
    "url": "^0.11.0"
  },
  "homepage": "./",
  "scripts": {
    "build:style": "tailwind build src/styles/index.css -o src/styles/tailwind.css",
    "start": "npm run build:style && react-scripts start",
    "build": "npm run build:style && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently --kill-others \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "build --em.main=build/electron.js",
    "preelectron-pack": "npm run build"
  },
  "build": {
    "appId": "com.plutooffice.electron-cra",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "concurrently": "^5.2.0",
    "electron": "^8.2.4",
    "electron-builder": "^22.5.1",
    "electron-react-devtools": "^0.5.3",
    "electron-reload": "^1.5.0",
    "postcss-cli": "^7.1.0",
    "wait-on": "^4.0.2"
  }
}
