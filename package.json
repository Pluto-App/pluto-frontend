{
  "name": "pluto-office",
  "productName": "pluto-office",
  "version": "0.1.1",
  "main": "public/electron.js",
  "author": "Pluto Dev Team <lahiri.devs@gmail.com>",
  "email": "lahiri.dev@gmail.com",
  "description": "pluto-office-app",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "animate.css": "^3.7.0",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "electron-disable-minimize": "^2.0.2",
    "electron-is-dev": "^1.2.0",
    "electron-titlebar": "0.0.3",
    "firebase": "^7.14.4",
    "lodash": "^4.17.15",
    "overmind": "^23.1.0",
    "overmind-react": "^24.1.0",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-remove-scroll-bar": "^2.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-show-more-list": "^0.5.2",
    "react-spinners": "^0.8.3",
    "react-toastify": "^6.0.4",
    "react-transition-group": "^4.4.1",
    "socket.io-client": "^2.3.0",
    "tailwindcss": "^1.4.6",
    "url": "^0.11.0"
  },
  "homepage": "./",
  "build": {
    "appId": "com.pluto.office",
    "compression": "normal",
    "productName": "pluto-office",
    "files": ["./build/**/*", "./node_modules/**/*", "./src/assets/**/*"],
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "mac": {
      "icon": "./assets/macos/logo.icns",
      "type": "distribution",
      "target": ["pkg", "tar.gz", "mas", "dmg"],
      "publish": {
        "provider": "github"
      }
    },
    "mas": {
      "entitlements": "./assets/entitlements.mas.plist"
    },
    "win": {
      "target": ["nsis", "nsis-web", "portable"],
      "icon": "./assets/windows/logo.ico",
      "publish": {
        "provider": "github"
      }
    },
    "linux": {
      "icon": "./assets/logo.png",
      "target": ["snap", "deb", "AppImage"],
      "description": "pluto-office",
      "category": "office",
      "publish": {
        "provider": "github"
      }
    }
  },
  "scripts": {
    "build:style": "tailwind build src/styles/index.css -o src/styles/tailwind.css",
    "start": "npm run build:style && react-scripts start",
    "build": "npm run build:style && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently --kill-others \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "dist": "NODE_ENV=production npm run build && electron-builder build --mac --win --linux --publish=always"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "concurrently": "^5.2.0",
    "electron": "^8.3.0",
    "electron-builder": "^22.6.0",
    "electron-packager": "^14.2.1",
    "electron-react-devtools": "^0.5.3",
    "electron-reload": "^1.5.0",
    "postcss-cli": "^7.1.1",
    "wait-on": "^4.0.2"
  }
}
