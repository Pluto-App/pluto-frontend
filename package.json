{
  "name": "plutoapp-new-frontend",
  "productName": "Pluto App",
  "version": "0.1.0",
  "main": "public/electron.js",
  "author": "Pluto Dev Team <lahiri.devs@gmail.com>",
  "email": "lahiri.dev@gmail.com",
  "description": "Pluto Office App",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "animate.css": "^3.7.0",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "electron-disable-minimize": "^2.0.2",
    "electron-is-dev": "^1.2.0",
    "electron-titlebar": "0.0.3",
    "firebase": "^7.14.2",
    "lodash": "^4.17.15",
    "overmind": "^23.0.1",
    "overmind-react": "^24.0.1",
    "qs": "^6.9.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-remove-scroll-bar": "^2.1.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-show-more-list": "^0.5.2",
    "react-spinners": "^0.8.1",
    "react-toastify": "^6.0.0-rc.2-1",
    "react-transition-group": "^4.4.0",
    "socket.io-client": "^2.3.0",
    "tailwindcss": "^1.3.5",
    "url": "^0.11.0"
  },
  "homepage": "./",
  "scripts": {
    "build:style": "tailwind build src/styles/index.css -o src/styles/tailwind.css",
    "start": "npm run build:style && PORT=4000 react-scripts start",
    "build": "npm run build:style && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently --kill-others \"BROWSER=none npm start\" \"wait-on http://localhost:4000 && electron .\"",
    "electron-pack": "electron-builder build -ml -c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "npm run build",
    "dist": "electron-builder install-app-deps && electron-builder",
    "exec-start": "electron-builder start",
    "electron-start": "electron ./build",
    "package": "npm run build && electron-packager build pluto-app --all"
  },
  "build": {
    "appId": "com.plutooffice.electron-cra",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "src/assets"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "target": [
        {
          "target": "deb"
        }
      ]
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "concurrently": "^5.2.0",
    "electron": "^8.2.4",
    "electron-builder": "^22.6.0",
    "electron-packager": "^14.2.1",
    "electron-react-devtools": "^0.5.3",
    "electron-reload": "^1.5.0",
    "postcss-cli": "^7.1.0",
    "wait-on": "^4.0.2"
  }
}
